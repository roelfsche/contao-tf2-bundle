{
    "name": "lumturo/contao-tf2-bundle",
    "type": "contao-bundle",
    "description": "turm-fuer-zwei bundles",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Rolf Staege",
            "homepage": "https://github.com/roelfsche"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "4.4.*",
        "symfony/framework-bundle": "^3.3"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Lumturo\\TF2Bundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Lumturo\\TF2Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lumturo\\TF2Bundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/roelfsche/contao-tf2-bundle/issues",
        "source": "https://github.com/roelfsche/contao-tf2-bundle"
    }
}
